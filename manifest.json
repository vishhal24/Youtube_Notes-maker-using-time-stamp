{
    "manifest_version": 3,
    "name": "YouTube Video Notepad",
    "version": "1.0",
    "description": "Take real-time notes on YouTube videos with timestamps and save them.",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  